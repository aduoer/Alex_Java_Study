<view class="list"wx:for="{{list}}"  
  wx:for-index="index"
  wx:for-item="item" wx:key="index">
  <view class="list_box" >
      <image class="list_box_left" src="{{item.img}}"/>
    <view class="list_box_right">
      <view class="title">
        {{item.title}}
      </view>
      <view class="time">
        {{item.time}}
      </view>
    </view>
  </view>
  <view class="empty"/>
</view>